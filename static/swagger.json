{
    "openapi": "3.0.1",
    "info": {
      "title": "My API",
      "version": "1.0.0",
      "description": "API for sending prompts to different models"
    },
    "paths": {
      "/api/get_responses": {
        "post": {
          "summary": "Send a prompt to multiple models",
          "description": "Sends a prompt to OpenAI and Google models and returns their responses.",
          "requestBody": {
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "prompt": {
                      "type": "string"
                    }
                  },
                  "required": ["prompt"]
                }
              }
            }
          },
          "responses": {
            "200": {
              "description": "Successful response",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "OpenAI": {
                        "type": "string"
                      },
                      "Google": {
                        "type": "string"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
  